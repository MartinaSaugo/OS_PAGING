#ifndef _SWAPSPACE_H_
#define _SWAPSPACE_H_

#include <types.h>
#include <current.h> //perch√® serve?
#include <vnode.h>
#include <addrspace.h>
#include <pagetable.h>

#define SWAPSIZE 9*1024*1024 //size of swap file
#define SWAPSLOTS SWAPSIZE/ PAGESIZE

//structs

struct swap_entry {
	struct addrspace *as;
	vaddr_t va;
}swap_entry_t;


//funcs

int swapinit(void);
int evictpage(struct pagetable_entry_t* page);

#endif
